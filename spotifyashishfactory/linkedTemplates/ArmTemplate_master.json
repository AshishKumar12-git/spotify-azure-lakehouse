{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifyashishfactory"
		},
		"ADLS_linked_Service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_linked_Service'"
		},
		"Sql_linked_service_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sql_linked_service'"
		},
		"ADLS_linked_Service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifyashishstorage.dfs.core.windows.net/"
		},
		"Sql_linked_service_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqldb-spotify.database.windows.net"
		},
		"Sql_linked_service_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotify_sqldb_oltp"
		},
		"Sql_linked_service_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "spotifyserver"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifyashishfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_linked_Service_accountKey": {
						"value": "[parameters('ADLS_linked_Service_accountKey')]"
					},
					"Sql_linked_service_password": {
						"value": "[parameters('Sql_linked_service_password')]"
					},
					"ADLS_linked_Service_properties_typeProperties_url": {
						"value": "[parameters('ADLS_linked_Service_properties_typeProperties_url')]"
					},
					"Sql_linked_service_properties_typeProperties_server": {
						"value": "[parameters('Sql_linked_service_properties_typeProperties_server')]"
					},
					"Sql_linked_service_properties_typeProperties_database": {
						"value": "[parameters('Sql_linked_service_properties_typeProperties_database')]"
					},
					"Sql_linked_service_properties_typeProperties_userName": {
						"value": "[parameters('Sql_linked_service_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}